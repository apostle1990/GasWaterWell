<Window x:Class="GasWaterWell.ImportGasWaterPhaseSeepage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GasWaterWell"
        mc:Ignorable="d"
        Title="导入气水相渗文件" Height="137.2" Width="453.2" 
        ResizeMode="NoResize" 
        WindowStartupLocation="CenterScreen">
    <Grid >
        <TextBlock  HorizontalAlignment="Left" Margin="10,31,0,0" TextWrapping="Wrap" Text="气水相渗名称：" VerticalAlignment="Top" Width="85" Height="21"/>
        <TextBox Name="gasWaterName" HorizontalAlignment="Left" Height="23" Margin="93,29,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="341"/>
        <Button Name="import" Click="ImportGasWater_Click" Content="导入"  HorizontalAlignment="Left" Margin="185,71,0,0" VerticalAlignment="Top" Width="75" Height="19"/>
        <Button Name="save" Click="Save_Click" Content="保存" HorizontalAlignment="Left" IsEnabled="False" Margin="272,71,0,0" VerticalAlignment="Top" Width="75" Height="19"/>
        <Button Name="cancelNew" Click="CancelNew_Click" Content="取消" HorizontalAlignment="Left" Margin="359,71,0,0" VerticalAlignment="Top" Width="75" Height="19"/>

    </Grid>
</Window>
